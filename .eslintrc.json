{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es2021": true,
    "mocha": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": ["error", "always"],
    "prefer-arrow-callback": "off",
    "linebreak-style": "off",
    "no-unused-vars": "off"
  }
}
